{
  "name": "@wokalski/vow",
  "description": "Almost sound Promises for Bucklescript",
  "keywords": [
    "reason",
    "bucklescript",
    "promise"
  ],
  "scripts": {
    "build": "bsb -make-world",
    "clean": "bsb -clean-world",
    "watch": "bsb -make-world -w",
    "docs:download-docre": "if [ ! -f ./docre.native ]; then curl -L https://github.com/jaredly/docre/releases/download/1.0-alpha1/docre.native -o docre.native && chmod 777 docre.native; fi",
    "docs:generate": "rm -rf ./docs && npm run docs:download-docre && ./docre.native ."
  },
  "repository": "wokalski/vow",
  "homepage": "https://github.com/wokalski/vow#readme",
  "bugs": "https://github.com/wokalski/vow/issues",
  "license": "MIT",
  "author": {
    "name": "Wojtek Czekalski",
    "email": "me@wczekalski.com",
    "url": "http://twitter.com/wokalski"
  },
  "version": "0.0.3",
  "devDependencies": {
    "bs-platform": "^2.2.3"
  }
}
